{
  "name": "ember-cp-validations",
  "version": "6.0.1",
  "description": "Ember computed property based validation library",
  "keywords": [
    "ember-addon",
    "ember-data",
    "validator",
    "validation",
    "model",
    "input",
    "form"
  ],
  "homepage": "https://github.com/adopted-ember-addons/ember-cp-validations",
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-cp-validations.git"
  },
  "license": "MIT",
  "author": "Offir Golan <offirgolan@gmail.com>",
  "contributors": [
    {
      "name": "Stefan Penner",
      "email": "stefan.penner@gmail.com"
    },
    {
      "name": "Ben Limmer",
      "email": "hello@benlimmer.com"
    },
    {
      "name": "Robert Jackson",
      "email": "me@rwjblue.com"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@embroider/macros": "~1.16.13",
    "ember-cli-babel": "^8.2.0",
    "ember-validators": "^4.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.3.1",
    "@embroider/test-setup": "^4.0.0",
    "@fortawesome/ember-fontawesome": "^1.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "broccoli-asset-rev": "^2.7.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~5.12.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "~4.12.8",
    "ember-inflector": "^6.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^12.0.1",
    "ember-set-helper": "^3.0.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "release-it": "^15.11.0",
    "release-plan": "^0.8.0",
    "sass": "^1.86.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "webpack": "^5.99.5"
  },
  "peerDependencies": {
    "ember-data": "*",
    "ember-source": ">= 4.0.0"
  },
  "peerDependenciesMeta": {
    "ember-data": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adopted-ember-addons.github.io/ember-cp-validations"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
