{
  "name": "@eflexsystems/ember-tracked-validations",
  "version": "6.0.0",
  "description": "Ember computed property based validation library",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "dependencies": {
    "ember-cached-decorator-polyfill": "^0.1.4",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-validators": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^1.0.0",
    "@fortawesome/ember-fontawesome": "^0.3.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "bootstrap": "^5.1.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.4",
    "ember-bootstrap": "^5.0.0",
    "ember-cli": "~4.4.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "~4.4.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~4.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "sass": "^1.49.0",
    "webpack": "^5.66.0",
    "yuidoc-ember-theme": "^2.0.1"
  },
  "keywords": [
    "ember-addon",
    "ember-data",
    "validator",
    "validation",
    "model",
    "input",
    "form"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/eflexsystems/ember-tracked-validations.git"
  },
  "license": "MIT",
  "author": "Jacob Jewell <jacobjewell@eflexsystems.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://offirgolan.github.io/ember-cp-validations"
  },
  "homepage": "https://github.com/offirgolan/ember-cp-validations"
}
